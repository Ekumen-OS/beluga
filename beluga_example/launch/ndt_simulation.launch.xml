<launch>
  <include file="$(find-pkg-share beluga_example)/launch/utils/ndt_amcl_launch.py">
    <arg name="use_sim_time" value="true"/>
    <arg name="localization_params_file" value="$(find-pkg-share beluga_example)/params/ndt_default.ros2.yaml"/>
    <arg name="localization_grid_map" value="$(find-pkg-share beluga_example)/maps/turtlebot3_world.hdf5"/>
  </include>

  <include file="$(find-pkg-share beluga_example)/launch/utils/rviz_launch.py">
    <arg name="display_config" value="$(find-pkg-share beluga_example)/rviz/rviz.ros2.rviz"/>
  </include>

  <include file="$(find-pkg-share beluga_example)/launch/utils/flatland_launch.py"/>
</launch>
